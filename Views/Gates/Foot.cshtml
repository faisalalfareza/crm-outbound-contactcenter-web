<script type="text/javascript">
    var time = { detik: 0, menit: 0, jam: 0 };
    var getTime = time.jam + ":" + time.menit + ":" + time.detik;
    $('.parameterForm').hide();
    $(".submitForm").hide();

    function jsFunction(value) {
        var number = $('.submitForm');

        number.show();

        var options = [];
        switch (value) {
            case "select": options = ["--Select Sub Status--"]; $('.parameterForm').hide(); $('.submitForm').fadeOut(500); break;
            case "Closing": options = ["Payment"]; $('.parameterForm').hide().delay(100).fadeIn(1000); break;
            case "Promising": options = ["Promising"]; $('.parameterForm').fadeOut(500); break;
            case "Prospect": options = ["Thinking"]; $('.parameterForm').fadeOut(500); break;
            case "Contacted": options = ["No deal", "Dispute", "Can't Located", "No time to talk", "leave Message", "Owner is different"]; $('.parameterForm').fadeOut(500); break;
            case "Connected": options = ["No Answer/ Busy/Voice Mail", "Rejected"]; $('.parameterForm').fadeOut(500); break;
            case "Not Connected": options = ["Invalid Number"]; $('.parameterForm').fadeOut(500); break;
            default: options = []; break;
        }

        var select = document.getElementById("transaction");
        //$("#transaction option").remove();
        while (select.hasChildNodes()) {
            select.removeChild(select.firstChild);
        }
        for (var i = 0; i < options.length; i++) {
            var opt = options[i];
            var el = document.createElement("option");
            el.textContent = opt;
            el.value = opt;
            select.appendChild(el);
        }
    }

    $(document).ready(function () {
        $('.date').datetimepicker({
            language: 'fr',
            weekStart: 1,
            todayBtn: 1,
            autoclose: 1,
            todayHighlight: 1,
            startView: 2,
            minView: 2,
            forceParse: 0
        });
    });

</script>
