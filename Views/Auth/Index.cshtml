@{
    ViewBag.Title = "CRM Outbound Contact Center";
}

<!-- Include Page Head -->
<!-- Bootstrap Plugin -->
<link href="~/Asset/css/bootstrap.min.css" rel="stylesheet">
<!-- Font Awesome -->
<link href="~/Asset/css/font-awesome/css/font-awesome.min.css" rel="stylesheet">
<!-- Bootstrap Material Design -->
<link href="~/Asset/css/app.css" rel="stylesheet">
<!-- build:css ../assets/css/app.min.css -->
<link href="~/Libs/bower/animate.css/animate.min.css" rel="stylesheet">
<link href="~/Libs/bower/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
<link href="~/Libs/bower/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
<link href="~/Libs/bower/material-design-iconic-font/dist/css/material-design-iconic-font.css" rel="stylesheet">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:400,500,600,700,800,900,300">
<!-- endbuild -->


<body class="simple-page">
    <div class="simple-page-wrap" style="margin-top:5%">
        
        <h1 id="_404_title" class="animated shake">404</h1>
        <h5 id="_404_msg" class="animated slideInUp">Oops, an error occur. The page can't be found</h5>
        <div id="_404_form" class="animated slideInUp">
            <form action="#">
                <div class="input-group">
                    <input type="search" class="form-control" placeholder="search">
                    <div class="input-group-addon"><i class="fa fa-search"></i></div>
                </div>
            </form>
        </div>

    </div><!-- .simple-page-wrap -->

    <!-- Include Page Foot -->
    <!-- Bootstrap Plugin js -->
    <script src="~/Asset/js/bootstrap.min.js" defer="defer"></script>
    <!-- build:js ../assets/js/core.min.js -->
    <script src="~/Libs/bower/jquery/dist/jquery.js" defer="defer"></script>
    <script src="~/Libs/bower/jquery-ui/jquery-ui.min.js" defer="defer"></script>
    <script src="~/Libs/bower/jQuery-Storage-API/jquery.storageapi.min.js" defer="defer"></script>
    <script src="~/Libs/bower/bootstrap-sass/assets/javascripts/bootstrap.js" defer="defer"></script>
    <script src="~/Libs/bower/superfish/dist/js/hoverIntent.js" defer="defer"></script>
    <script src="~/Libs/bower/superfish/dist/js/superfish.js" defer="defer"></script>
    <script src="~/Libs/bower/jquery-slimscroll/jquery.slimscroll.js" defer="defer"></script>
    <script src="~/Libs/bower/perfect-scrollbar/js/perfect-scrollbar.jquery.js" defer="defer"></script>
    <script src="~/Libs/bower/PACE/pace.min.js" defer="defer"></script>
    <!-- endbuild -->
    <!-- build:js ~/Asset/js/app.min.js -->
    <script src="~/Asset/js/fullcalendar.js" defer="defer"></script>
    <script src="~/Asset/js/library.js" defer="defer"></script>
    <script src="~/Asset/js/plugins.js" defer="defer"></script>
    <script src="~/Asset/js/app.js" defer="defer"></script>
    <!-- endbuild -->
    <script src="~/Libs/bower/moment/moment.js" defer="defer"></script>
    <script src="~/Libs/bower/fullcalendar/dist/fullcalendar.min.js" defer="defer"></script>





</body>